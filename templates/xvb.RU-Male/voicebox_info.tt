						<td>[% param.CALL_NOTE %]</td>
						<td align='left' nowrap='nowrap'>
							<a href='?action=multidialout_del&id=[% VBOX_PREF.ID %]&data_id=[% param.DATA_ID | html %]&uniq=[% USER_CREDS.UNIQ %]' onclick="return xmlhttpDel(this,'ru','d-tr-[% param.DATA_ID %]')" title='delete callblast item'><img border='0' src='/xvb/images/msg_delete.png' alt='удалить' /></a>&nbsp;
							<span class='in_t' id='in_t_id-[% param.DATA_ID %]'>
