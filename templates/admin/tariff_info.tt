			<td>DATETIME</td>
			<td><input class='in_t0' name='T_CALL_PRICE_E_DATETIME' type='text' value='[% CALL_PRICE_E_DATETIME | html %]'></td>
			<td><input class='in_t0' name='T_MINUTE_PRICE_E_DATETIME' type='text' value='[% MINUTE_PRICE_E_DATETIME | html %]'></td>
		</tr>
		
		<tr class='nocolor'>
