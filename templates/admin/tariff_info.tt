			<td><input class='in_t0' name='T_CALL_PRICE_E_DISA' type='text' value='[% CALL_PRICE_E_DISA | html %]'></td>
			<td><input class='in_t0' name='T_MINUTE_PRICE_E_DISA' type='text' value='[% MINUTE_PRICE_E_DISA | html %]'></td>
