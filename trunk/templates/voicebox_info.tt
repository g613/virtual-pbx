							[% ELSIF param.CALL_STATUS eq 'wait' %]
								wait	
							[% ELSIF param.CALL_STATUS %]
								<a class='error' href='?action=act_list&call_id=[% param.CALL_ID %]&uniq=[% USER_CREDS.UNIQ %]'>[% param.CALL_STATUS | html %]</a>
							[% ELSE %]
								unknown
							[% END %]
						</td>
						<td>[% param.DURATION | duration_str | html %]</td>
						<td>[% param.ATTEMPT %]</td>
						<td align='left' nowrap='nowrap'>
							<a href='?action=multidialout_del&id=[% VBOX_PREF.ID %]&data_id=[% param.DATA_ID | html %]&uniq=[% USER_CREDS.UNIQ %]' onclick="return xmlhttpDel(this,'en','d-tr-[% param.DATA_ID %]')" title='delete callblast item'><img border='0' src='/xvb/images/msg_delete.png' alt='del' /></a>&nbsp;
							<span class='in_t' id='in_t_id-[% param.DATA_ID %]'>
								<script language='JavaScript'>
									writeUpdateLinks( 'save changes' );
								</script>
								<noscript><input value='change' type='submit'></noscript>
							</span>
						</td>
						</form>
					</tr>
					[% END %]
					[% IF MSG_NEXT ne '' %]
					<tr>
						<td align='right' colspan='8'>
							<a title='next page' href='?action=vb_view&id=[% VBOX_PREF.ID %]&from=[% MSG_NEXT %]&uniq=[% USER_CREDS.UNIQ %]'><img border='0'  src='/xvb/images/msg_next.png' alt='[% MSG_PREV %] &gt;=' /></a>
						</td>
					</tr>
					[% END %]
					<tr align='center'>
						<form method="post" action="[% CGI_URL %]" onSubmit='return checkChanges(this)'>
