<html>
<head>
	<title>Virtual PBX - tenants</title>
	<link rel="stylesheet" type="text/css" href="/xvb/xvb.css" />
	<script language="JavaScript" src="/xvb/js/xvb.js"></script>
</head>

<body onLoad="XVBInit()">
	
	[% IF CGI_ERROR ne '' %]
	<script language="JavaScript" type="text/javascript">
	<!--
		alert('[% CGI_ERROR | html %]')
	//-->
	</script>
	<noscript>[% CGI_ERROR | html %]</noscript>
	[% END %]

	<table width='95%' class='logo'>
		<tr align='left'>
			<td width='10%'>
				<img border='0' src='/xvb/images/logo-small.png' alt='Virtual PBX' />
			</td>
			<td valign='middle'>
				[% INCLUDE main_menu.tt-inc %]
				[% INCLUDE main_menu.tt-inc-custom %]
			</td>
			<td align='right'>
				<a href='/xvb/XVB-AI.pdf?a=msg_download'><img border='0' src='/xvb/images/help.png' alt='about' /></a>
				[% INCLUDE version.tt %]
			<td>
		</tr>
	</table>

	<center>
	<h1>Tenants</h1>
	
	<table width='100%'>
		<tr>
			<form method="post" onSubmit='return checkChanges(this)'>
			<td align='right'>
				<input placeholder='shared DID' size='12' name='f_vn' type='text' value='[% F_VN | html %]'>
				&nbsp;
				<input placeholder='Access code' size='12' name='f_ac' type='text' value='[% F_AC | html %]'>
				&nbsp;
				<input placeholder='User name' size='25' name='f_uname' type='text' value='[% F_UNAME | html %]'>
				&nbsp;
				<select name="f_group" id="group_type_selector">
					<option value="" selected>any group</option>
					[% FOREACH gr = GROUPS %]
						<option value="[% gr.GROUP_ID | html %]">[% gr.GROUP_NAME | html %]</option>
					[% END %]
				</select>
				&nbsp;
				<select name="f_status" id="status_type_selector">
				   	<option value="" selected>Any</option>
					<option value="1">Active</option>
					<option value="0">Locked</option>
				</select>
				&nbsp;
				<input type='hidden' value='user_list' name='action'>
				&nbsp;
				<input value='search' type='submit'>
			</td>
			<script language='JavaScript'>
				setActiveSelector('status_type_selector',"[% F_STATUS %]");
				setActiveSelector('group_type_selector',"[% F_GROUP %]");
			</script>
			</form>
		</tr>
	</table>

	<p>
	<table width='95%' id='list-data'>
		<tr>
			<th>Shared DID</th>
			<th>Access code</th>
			<th>User name</th>
			<th>Language</th>
			<th>Group</th>
			<th>Status</th>
			<th>R/O</th>
			<th>Tariff</th>
			<th>Balance</th>
			<th>Actions</th>
		</tr>
			
		<tr align='center' class='nocolor'>
			<td align='left' colspan='10'>
				[% IF MSG_PREV ne '' %]
				<a title='previous page' href='?action=user_list&from=[% MSG_PREV %]&f_uname=[% F_UNAME | html %]&f_ac=[% F_AC %]&f_vn=[% F_VN %]&f_group=[% F_GROUP %]&f_status=[% F_STATUS %]'><img border='0'  src='/xvb/images/msg_prev.png' alt='[% MSG_PREV %] &lt;=' /></a>
				[% END %]
			</td>
		</tr>

		[% FOREACH us = USERS %]
		<tr align='center' class='nocolor'>
			<td align='right'>
				[% us.VOICENUMBER | html %]
			</td>
			<td>
				<a title='view tenant profile' href='ui?phone=[% us.VOICENUMBER %]&ac=[% us.ACCESS_CODE %]&pw=[% us.PIN_CODE %][% us.PIN_CODE2 %]&action=start&saction=user_info'>[% us.ACCESS_CODE | html %]</a>
			</td>
			<td align='left'>
				[% us.FIRST_NAME | html %] [% us.LAST_NAME | html %]
			</td>
			<td align='left'>
				[% us.LANG_NAME | html %]
			</td>
			<td>
				<a title='group info' href='?action=group_info&group_id=[% us.GROUP_ID %]'>[% us.GROUP_NAME | html %]</a>
			</td>
			[% IF us.STATUS %]
			<td class='success'>
				active
			[% ELSE %]
			<td class='error'>
				locked
			[% END %]
			</td>
			<td class='ext_pv'>
				[% IF us.DEMO_MODE %]
					+
				[% ELSE %]
					-
				[% END %]
			</td>
			<td>
				[% us.TARIFF_NAME | html %]
			</td>
			<td align='right'>
			[% IF USE_BILLING %]
				[% us.USER_BALANCE %] [% us.CURRENCY_NAME | html %]
			[% ELSE %]
				-
			[% END %]
			</td>
			<td>
				<a title='delete tenant' href='?action=user_del&user_id=[% us.ID %]&from=[% MSG_FROM %]&f_ac=[% F_AC %]&f_vn=[% F_VN %]&f_group=[% F_GROUP %]&f_status=[% F_STATUS %]' onclick="return DelConfirm('en')"><img border='0'  src='/xvb/images/vb_del.png' alt='del' /></a>&nbsp;&nbsp;
				<a title='edit tenant settings' href='?ac=[% us.ACCESS_CODE %]&action=user_info'><img border='0'  src='/xvb/images/vb_edit.png' alt='tenant info' /></a>
			</td>
		</tr>
		[% END %]
		[% IF MSG_NEXT ne '' %]
		<tr>
			<td align='right' colspan='10'>
				<a title='next page' href='?action=user_list&from=[% MSG_NEXT %]&f_uname=[% F_UNAME | html %]&f_ac=[% F_AC %]&f_vn=[% F_VN %]&f_group=[% F_GROUP %]&f_status=[% F_STATUS %]'><img border='0'  src='/xvb/images/msg_next.png' alt='=&gt; [% MSG_NEXT %]' /></a>
			</td>
		</tr>
		[% END %]
	</table>
	<script language='JavaScript'>listcolorer( 'list-data' );</script>
	</p>
	
	<p>
	<table width='95%'>
		<form method="post" onSubmit='return checkChanges(this)'>
		<tr align='left'>
			<td>
				&nbsp;
				Shared DID: 
				<input size='11' type='text' name='phone'>
				&nbsp;
				Group: 
				<select name="group_id">
				[% FOREACH gr = GROUPS %]
					<option value="[% gr.GROUP_ID | html %]">[% gr.GROUP_NAME | html %]</option>
				[% END %]
				</select>
				&nbsp;
				[% IF TEMPLATES scalar %]
				Template: 
				<select name="conf_file">
				[% FOREACH tmpl = TEMPLATES %]
					<option value="[% tmpl.FILE | html %]">[% tmpl.DESC | html %]</option>
				[% END %]
				</select>
				&nbsp;
				[% END %]
				<input value='&nbsp;&nbsp;Add tenant&nbsp;&nbsp;' type='submit'>
				<input type='hidden' value='user_add' name='action'>
			</td>
		</tr>
		</form>
	</table>
	</p>

	</center>
	
	[% INCLUDE main_menu2.tt-inc-custom %]
	
	<div id='shadow'><div id='loading'></div></div><div id='center'></div>
	</body>
</html>
			
